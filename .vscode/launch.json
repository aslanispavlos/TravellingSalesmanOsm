{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
		"name": "lldb Launch",
		"type": "cppdbg",
		"request": "launch",
		"program": "${workspaceFolder}/src/debug.out",
		"args": ["${env:OSM_DATA_DIR}/hamburg-latest.osm.pbf", "50", "2", "400", "800", "2014", "4039"],
		"stopAtEntry": false,
		"cwd": "${workspaceFolder}/src/",
		"environment": [],
		"externalConsole": false,
		"preLaunchTask": "make debug",
		"osx": {
			"MIMode": "lldb",
		}
	}, {
		"name": "lldb webserver",
		"type": "cppdbg",
		"request": "launch",
		"program": "${workspaceFolder}/src/main.out",
		"args": ["${env:OSM_DATA_DIR}/hamburg-latest.osm.pbf"],
		"stopAtEntry": false,
		"cwd": "${workspaceFolder}/src/",
		"environment": [],
		"externalConsole": false,
		"preLaunchTask": "make webserver",
		"osx": {
			"MIMode": "lldb",
		}
	}]
}
